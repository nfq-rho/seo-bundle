{
    "name": "nfq-rho/seo-bundle",
    "description": "SEO URL and meta information integration bundle.",
    "keywords": ["NFQ", "SEO"],
    "type": "symfony-bundle",
    "require": {
        "php": ">=7.2",
        "symfony/symfony": "~3.1|~4.0",

        "doctrine/orm": "~2.5",
        "doctrine/doctrine-bundle": "~1.4",
        "behat/transliterator": "~1.1",
        "tedivm/stash-bundle": "0.4.*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "Nfq\\SeoBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
